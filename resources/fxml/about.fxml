<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane stylesheets="@../css/styles.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AboutController">
   <children>
      <ImageView fx:id="imageView" fitHeight="500.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/aboutFront.png" />
         </image>
      </ImageView>
      <Button fx:id="frontBtn" layoutY="474.0" mnemonicParsing="false" onAction="#flipToBack" prefHeight="24.0" prefWidth="24.0" styleClass="buttons" AnchorPane.bottomAnchor="4.0" AnchorPane.leftAnchor="2.0" />
      <Button fx:id="backBtn" disable="true" layoutX="141.0" layoutY="198.0" mnemonicParsing="false" onAction="#flipToFront" prefHeight="92.0" prefWidth="229.0" styleClass="buttons" />
   </children>
</AnchorPane>
